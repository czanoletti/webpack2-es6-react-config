{
  "name": "webpack-js-jsx",
  "version": "1.0.0",
  "description": "Configuration of webpack for compiling react or any es2015 code and karma for testing",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "node ./app/server.js",
    "start-dev": "webpack-dev-server --inline --progress --port 8080",
    "webpack": "webpack --watch",
    "test": "karma start",
    "build": "mkdir ./app/public/void && rm -r ./app/public/** && webpack --config config/webpack.prod.js --progress --profile --bail"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "jquery": "^3.2.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.26.1",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.16.1",
    "image-webpack-loader": "^3.3.1",
    "import-loader": "^1.0.1",
    "jasmine-core": "^2.6.3",
    "json-loader": "^0.5.4",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "null-loader": "^0.1.1",
    "postcss-loader": "^2.0.5",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^2.0.2",
    "rimraf": "^2.5.2",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.13.1",
    "typescript": "~2.0.10",
    "url-loader": "^0.5.8",
